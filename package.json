{
  "main": "./dist/extension.cjs",
  "module": "index.ts",
  "dependencies": {
    "@types/vscode": "^1.86.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@vscode/vsce": "^2.23.0",
    "esbuild": "^0.20.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "echo 'Open this directory in VSCode and then run your extension with F5 or `Run and Debug > Run Extension`!'",
    "build": "bun run ./scripts/build-with-esbuild.ts",
    "watch": "bun run ./scripts/watch-with-esbuild.ts",
    "vsce-package": "bun run vsce package",
    "package": "bun run build && bun run vsce-package",
    "clean": "rm -r ./dist"
  },
  "//": "START EXTENSION ATTRIBUTES",
  "name": "vscode-console-log-value",
  "author": "lalunamel",
  "publisher": "lalunamel",
  "version": "0.0.1",
  "description": "Log a value and it's name using console.log",
  "displayName": "Console Log Value",
  "repository": {
    "type": "git",
    "url": "https://github.com/lalunamel/vscode-console-log-value"
  },
  "license": "MIT",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Snippets"
  ]
}
